{
    "name": "wyang14/chatter",
    "description": "A simple forum package for your laravel app based on devdojo/chatter",
    "authors": [
        {
            "name": "Weili Yang",
            "email": "william.1022@gmail.com"
        }
    ],
    "require": {
        "luketowers/purifier": "~3.0"
    },
    "require-dev": {
        "illuminate/cache": "~5.1.20|5.2.*|5.3.*",
        "illuminate/console": "~5.1.20|5.2.*|5.3.*",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "~5.7.14"
    },
    "autoload": {
	    "psr-4": {
	      "Wyang14\\Chatter\\": "/src"
		}
	},
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wyang14\\Chatter\\ChatterServiceProvider"
            ]
        }
    }
}
